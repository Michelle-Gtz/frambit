generator client {
  provider = "prisma-client-js"
  output   = "../generated/client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(cuid())
  name      String?
  email     String   @unique
  clerkId   String   @unique
  createdAt DateTime @default(now())
  image     String?
  updatedAt DateTime @updatedAt
}
